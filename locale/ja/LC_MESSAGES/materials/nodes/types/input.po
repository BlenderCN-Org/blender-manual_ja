msgstr ""

#: ../../manual/materials/nodes/types/input.rst:140
msgid "Amount of reflectiveness of the object."
msgstr ""

#: ../../manual/materials/nodes/types/input.rst:141
msgid "Alpha"
msgstr ""

#: ../../manual/materials/nodes/types/input.rst:142
msgid ""
"Transparency of the material by setting all pixels in the alpha channel to "
"the given value."
msgstr ""

#: ../../manual/materials/nodes/types/input.rst:145
msgid "Translucency"
msgstr ""

#: ../../manual/materials/nodes/types/input.rst:144

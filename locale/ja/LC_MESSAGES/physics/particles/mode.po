msgstr ""

#: ../../manual/physics/particles/mode.rst:174
msgid "No special tool, just edit the keypoints as \"normal\" vertices."
msgstr ""

#: ../../manual/physics/particles/mode.rst:175
msgid "Comb"
msgstr ""

#: ../../manual/physics/particles/mode.rst:176
msgid "Moves the keypoints (similar to \"proportional editing\")."

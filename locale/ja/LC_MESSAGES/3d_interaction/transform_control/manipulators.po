msgid ""
":guilabel:`A`: Standard cube in default top view with *global* orientation "
"selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:116
msgid ""
":guilabel:`B`: Standard cube with view rotated and *global* orientation "
"selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:117
msgid ""
":guilabel:`C`: Randomly rotated cube with view rotated and *global* "
"orientation selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:118
msgid ":guilabel:`D`: Randomly rotated cube with *local* orientation selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:119
msgid ""
":guilabel:`E`: Randomly rotated cube with *gimbal* orientation selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:120
msgid ""
":guilabel:`F`: Randomly rotated cube with *normal* orientation selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:121
msgid ""
":guilabel:`G`: Randomly rotated cube, vertices selected with *normal* "
"orientation selected"
msgstr ""

#: ../../manual/3d_interaction/transform_control/manipulators.rst:122
msgid ":guilabel:`H`: Randomly rotated cube with *view* orientation selected"
msgstr ""

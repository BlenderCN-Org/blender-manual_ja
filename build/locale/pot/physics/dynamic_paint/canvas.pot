msgstr ""

#: ../../../manual/physics/dynamic_paint/canvas.rst:129
# c0c6a0fe746941f0851e14ed216e66b6
msgid "Timescale"
msgstr ""

#: ../../../manual/physics/dynamic_paint/canvas.rst:128
# c392b84bbbd04391863f72f59771c314
msgid "Directly adjusts simulation speed without affecting simulation outcome. Lower values make simulation go slower and otherwise."
msgstr ""


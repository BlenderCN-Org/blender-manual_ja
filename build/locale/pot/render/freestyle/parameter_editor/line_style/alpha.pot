msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:53
# 7001b7c3111849c9b1ea25dd52d5a350
msgid "Line Style Alpha's Distance From Camera modifier"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:56
# 9f410db166f54b73b34ad735d2ec8e9b
msgid "The *Distance from Camera* modifier alters the base alpha with a new one from either a linear progression or a custom curve, using the distance to the active camera as parameter."
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:68
#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:98
# df097a291f8949af98f98d09fc0d839c
# c338911a9dcf4f51a33e391a3985dfaa
msgid "Range Min and Range Max"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:64
# 96bd28fc498d4d06bdcd2d4fc299f263
msgid "The limits of the mapping from \"distance to camera\" to \"alpha in mapping\". If the current point of the stroke is at *Range Min* or less from the active camera, it will take the start alpha of the mapping, and conversely, if it is at *Range Max* or more from the camera, it will take the end alpha of the mapping. These values are in the current scene's units, not in pixels!"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:72
#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:102
# 195797c482b84bef86d4fd2082ae3b24
# 6db0391c38a94a5dadb41fc8e5f11ccf
msgid "Fill Range by Selection"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:71
# 407018fb1f5746f0893a4e24492268d3
msgid "Set the min/max range values from the distances between the current selected objects and the camera."
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:75
# c649f13ddd4849ed9c4b3635c0700b0b
msgid "Distance from Object"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:80
# 38e48fea471e4c4eaa59c0de4e3f1b20
msgid "Line Style Alpha's Distance From Object modifier"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:83
# edbeb0050b0e4c4bbbbe487bd60457a1
msgid "The *Distance from Object* modifier alters the base alpha with a new one from either a linear progression or a custom curve, using the distance to a given object as parameter."
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:87
# a8f5414a29f84adc8efc9523674fbc6b
msgid "Target"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:87
# 832e17c9d221412da7a6fe9fcb2dd491
msgid "The object to measure distance from."
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:94
# 3da2702f39e541fd965431376071c001
msgid "The limits of the mapping from \"distance to object\" to \"alpha in mapping\". If the current point of the stroke is at *Range Min* or less from the target, it will take the start alpha of the mapping, and conversely, if it is at *Range Max* or more from the target, it will take the end alpha of the mapping. These values are in the current scene's units, not in pixels!"
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:101
# 1a3fcf70a2574811bc5b795280c1b19a
msgid "Set the min/max range values from the distances between the current selected objects and the target."
msgstr ""

#: ../../../manual/render/freestyle/parameter_editor/line_style/alpha.rst:105

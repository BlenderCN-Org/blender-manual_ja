msgid "*Sphere* enabled with the specified distances, *Inverse Linear* light falloff."
msgstr ""

#: ../../../manual/render/blender_render/lighting/lights/light_attenuation.rst:465
# ef4390f2b9094709b1621570b4d7103e
msgid "Sphere with 10."
msgstr ""


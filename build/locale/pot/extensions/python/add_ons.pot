msgstr ""

#: ../../../manual/extensions/python/add_ons.rst:14
# ccf52edfee6544d4981d6b197a9ba6b3
msgid "Searching"
msgstr ""

#: ../../../manual/extensions/python/add_ons.rst:16
# 0885eeb2b8dd46c1bb5dd076773a3763
msgid "Blender comes with some useful Add-ons already, ready to be enabled, but you can also add your own, or any interesting ones you find on the web."
msgstr ""

#: ../../../manual/extensions/python/add_ons.rst:22
# 65cdf9ac07324036a8a8b693bd95267e
msgid "Add-ons tab in the User Preferences"
msgstr ""

#: ../../../manual/extensions/python/add_ons.rst:26
# a576f076e10a4abc8f8c12295c4760b4
msgid "The `Scripts Catalog <http://wiki.blender.org/index.php/Extensions:2.6/Py/Scripts>`__ provides an index of Add-ons that are included with Blender as well as listing a number of external Add-ons."
msgstr ""


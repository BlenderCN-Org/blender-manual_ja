msgid "RLE, runlength encoded, lossless, works well when scanlines have same values."
msgstr ""

#: ../../../manual/data_system/files/formats/openexr.rst:49
# 8f4c1428de994a1fa0f2ae8bbc45dad7
msgid "PXR24. lossy algorithm from Pixar, converting 32 bits floats to 24 bits floats."
msgstr ""


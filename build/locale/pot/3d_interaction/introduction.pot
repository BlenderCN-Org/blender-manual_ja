msgid "Left-right, commonly called the \"\\ *x* \" axis."
msgstr ""

#: ../../../manual/3d_interaction/introduction.rst:9
# 893c0367f92349ecae6328babb20056e
msgid "Forward-backward, commonly called the \"\\ *y* \" axis."
msgstr ""

#: ../../../manual/3d_interaction/introduction.rst:10
# 627db660f58949229af042be29270974

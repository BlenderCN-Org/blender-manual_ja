msgstr ""

#: ../../../manual/preferences/system.rst:62
# c49bf0649e9444dba867b73c699140ca
msgid "*512* , *1024* , *2048* (Default), *4096* , *8192*, *16384*, and *32768*"
msgstr ""

#: ../../../manual/preferences/system.rst:64
# 537c871fd12741eca8bb4100f5a022b4
msgid "Sample Rate"
msgstr ""

#: ../../../manual/preferences/system.rst:64
# cb1104ecb847494f9420101c32d8c68a
msgid "Set the audio sample rate. Available options are: *44.1 Khz* (Default), *48 Khs* , *96 Khz* and *192Khz*"
msgstr ""

#: ../../../manual/preferences/system.rst:70
# 2f8ba9f0ac544aecb373322a279de5f3
msgid "Sample Format"
msgstr ""

#: ../../../manual/preferences/system.rst:67
# a46c9695fe88475ebad462526ad64f84
msgid "Set the audio sample format. Available options are: *32 bit float* (Default), *8 bit Unsigned* , *16 Bits Signed* , *24 Bits Signed* , *32 Bits Signed* , *32 Bits Float* and *64 Bits Float*"
msgstr ""

#: ../../../manual/preferences/system.rst:73
# 15c7cc5041ce47dab6ffa6664ba81294
msgid "Screencast"
msgstr ""

#: ../../../manual/preferences/system.rst:75
# 2011c5f6b47a4f2284c08592d6a05c3e
msgid "TODO"

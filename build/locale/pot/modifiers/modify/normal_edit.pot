#: ../../../manual/modifiers/modify/normal_edit.rst:4
# d6b4b295fda44ca3a56fc089841d3e01
msgid "Normal Edit Modifier"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:6
# 3e3fa35bdf0c4ffc9807972691379f5a
msgid "The **Normal Edit** modifier affects (or generates) custom normals. It uses a few simple parametric methods to compute normals (quite useful in game development and architecture areas), and mixes back those generated normals with existing ones."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:14
# 880cb98daf264a13afa73bfa6e22cfde
msgid "Options"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:18
# 6bc9f69a156d4f19958df27e8ff0ca1a
msgid "Normal Edit modifier."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:26
# 56783bf4b7d34c76ae7a38b08381ccb7
msgid "Radial/Directional"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:21
# c2356bcbdd844b0e99b650f962935da2
msgid "The two modes currently available to generate normals."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:23
# 45f9bcfe5eed4a32899f4033a7f47cbc
msgid "Radial aligns normals with the (origin, vertex coordinates) vector, in other words all normals seems to radiate from the given center point, as if they were emitted from an ellipsoid surface."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:26
# b3abf2e93f5842ce83218edeb2e44fc9
msgid "Directional makes all normals point (converge) towards a given target object."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:31
# 589aac6c2c1e4e3b9222babcfee02a78
msgid "Target Object"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:29
# 6b7b4ed5ae6e4691bc70196d16f1cb93
msgid "Uses this object’s center as reference point when generating normals."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:31
# 3a34ee4085be4fa6ae16abdc5ecb6cb9
msgid "Optional in *Radial* mode, mandatory in *Directional* one."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:37
# 2bfc2cccd4ef4d42aa8d9825b30dcf7f
msgid "Parallel Normals"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:34
# e6eb29065c42472298f2ab16f68b45f9
msgid "Makes all normals parallel to the line between both objects’ centers, instead of converging towards target’s center."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:37
# 39c62f453f9c4692be374425f5a81058
msgid "Only relevant in *Directional* mode"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:43
# 3a3c264f329742218ff2949977f4a0ee
msgid "Offset"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:40
# a9a088368d2d4cb381eaacf9c1ccda6f
msgid "Gives modified object’s center an offset before using it to generate normals."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:42
# 7952c2eb18544bd8a417f270d1521d73
msgid "Only relevant in *Radial* mode if no *Target Object* is set, and in *Directional* mode when *Parallel Normals* is set."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:48
# ecbfedfd63ce44d4b108098c7ad85c98
msgid "Mix Mode"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:46
# 790103af3d134e4981d81f0e132436ae
msgid "How to affect existing normals with newly generated ones."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:48
# 2c6df0985b104d66a9320b550bda7dfc
msgid "Note the *Multiply* option is **not** a cross product, but a mere component-by-component multiplication."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:51
# 6848e5c2440449a5b0a0a49cc46510d8
msgid "Mix Factor"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:51
# 7d76cdb9cddf4fbe8705372e131ac82f
msgid "How much of the generated normals get mixed into existing ones."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:55
# b1f378fcb7884415aa80d7a21da840eb
msgid "Vertex Group"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:54
# f158fe781a3941d4906d296221cf43b0
msgid "Allows per-item fine control of the mix factor. Vertex group influence can be reverted using the small “arrow” button to the right."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:58
# 3e463ab8d46b40b29fb6f53d05e5c903
msgid "Usage"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:60
# f007cd3691d44046843e47601b1f275a
msgid "This modifier can be used to quickly generate radial normals for low-poly tree foliage, or “fix” shading of toon-like rendering by partially bending default normals…"
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:63
# f7f5b4bcbb0248e0b9a21a422af4045e
msgid "The only mandatory prerequisite to use it is to enable *Auto Smooth* option in Mesh properties, *Normals* panel."
msgstr ""

#: ../../../manual/modifiers/modify/normal_edit.rst:66
# e35cffed798e48a9b54d49bb4d31c195
msgid "More complex normal manipulations can be achieved by copying normals from one mesh to another, see the :doc:`Data Transfer modifier </modifiers/modify/data_transfer>`."
msgstr ""


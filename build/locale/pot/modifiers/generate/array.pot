# 5cb2ff0bc4b341bb9c8eba84fc14a9e0
msgid "Constant Offset, X, Y, Z"
msgstr ""

#: ../../../manual/modifiers/generate/array.rst:56
# 1d4ff36e4ba241449e32205681931dc4
msgid "Adds a constant translation component to the duplicate object's offset. X, Y and Z constant components can be specified."
msgstr ""

#: ../../../manual/modifiers/generate/array.rst:66
# c694c5b831944f76aecf68879e80372c
msgid "Relative Offset, X, Y, Z"
msgstr ""

#: ../../../manual/modifiers/generate/array.rst:62

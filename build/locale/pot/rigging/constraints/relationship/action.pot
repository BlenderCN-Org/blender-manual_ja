msgstr ""

#: ../../../manual/rigging/constraints/relationship/action.rst:57
# 4039db09a53544ffa30cd4d039718c4b
msgid "Target Space"
msgstr ""

#: ../../../manual/rigging/constraints/relationship/action.rst:57
# 73a7487ca51b43bda3760d2a1c3d8ae4
msgid "This constraint allows you to choose in which space to evaluate its target's transform properties."
msgstr ""

#: ../../../manual/rigging/constraints/relationship/action.rst:66
# 187ce8ceb6d34887a20879c69281bb92
msgid "To Action"
msgstr ""

#: ../../../manual/rigging/constraints/relationship/action.rst:60
